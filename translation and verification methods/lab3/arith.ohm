Arithmetic {
    Expr = Sum
    SumOperator = "+" | "-"
    Sum = Mul (SumOperator Mul)*
    MulOperator = "*" | "/"
    Mul = Unary (MulOperator Unary)*
    Unary = 
    "-" Unary  -- neg 
    | Primary
    Primary = number        -- num
        | variable      -- var
        | "(" Expr ")"  -- parens

    variable = letter alnum*
    nonZeroDigit = "1".."9"
    number
        = "0"                  -- zero
        | nonZeroDigit digit*  -- nz
}
